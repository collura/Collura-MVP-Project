<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.collura.mvpstudy">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="example.collura.mvpstudy.mvp.mainactivity.AppMainActivity"
            android:label="Main Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="example.collura.mvpstudy.mvp.secondactivity.AppSecondActivity"
            android:label="Second Activity"
            android:parentActivityName="example.collura.mvpstudy.mvp.mainactivity.AppMainActivity"/>
    </application>

</manifest>